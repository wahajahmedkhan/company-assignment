{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "check": "npm run check:style && npm run check:code",
    "check:style": "prettier --check .",
    "check:code": "eslint .",
    "fix": "npm run fix:style && npm run fix:code",
    "fix:code": "prettier --write .",
    "fix:style": "eslint --fix .",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.2.0",
    "@angular/cdk": "13.2.3",
    "@angular/common": "13.2.0",
    "@angular/compiler": "13.2.0",
    "@angular/core": "13.2.0",
    "@angular/fire": "^7.4.1",
    "@angular/forms": "13.2.0",
    "@angular/localize": "13.2.0",
    "@angular/material": "13.2.3",
    "@angular/platform-browser": "13.2.0",
    "@angular/platform-browser-dynamic": "13.2.0",
    "@angular/router": "13.2.0",
    "rxjs": "7.5.4",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.2.3",
    "@angular-eslint/builder": "13.4.0",
    "@angular-eslint/eslint-plugin": "13.4.0",
    "@angular-eslint/eslint-plugin-template": "13.4.0",
    "@angular-eslint/schematics": "13.4.0",
    "@angular-eslint/template-parser": "13.4.0",
    "@angular/cli": "13.2.3",
    "@angular/compiler-cli": "13.2.0",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.18",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "8.3.0",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.1",
    "json-server": "0.17.0",
    "prettier": "2.5.1",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  }
}
